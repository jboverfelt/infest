create_table("closures", func(t) {
	t.Column("id", "uuid", {"primary": true})
	t.Column("name", "text", {})
	t.Column("address", "text", {})
	t.Column("reason", "text", {})
	t.Column("closureDate", "timestamp", {})
	t.Column("reopenDate", "timestamp", {"null": true})
	t.Column("reopenComments", "string", {"null": true})
})